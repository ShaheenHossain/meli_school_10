<odoo>
	<data>
		<record id="complete_survey_tree_view" model='ir.ui.view'>
			<field name="name">Survey Form</field>
			<field name="model">complete.survey</field>
			<field name="arch" type='xml'>
				<tree>
					<field name="name"/>
					<field name="campus"/>
					<field name="level_id"/>
					<field name="class_id"/>
					<field name="date"/>
					<field name="strength"/>
					<field name="survey_student"/>
		
				</tree>
			</field>
		</record>
		<record id="complete_survey_form_tree" model="ir.ui.view">
			<field name="name">Survey Form</field>
			<field name="model">complete.survey</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<group>
								<field name="name"/>
								<field name="campus"/>
								<field name="program"/>
								<field name="level_id" domain="[('standard_id','=',program)]" widget="selection"/>
							</group>
							<group>
								<field name="shift_id" domain="[('program','=',program)]" widget="selection"/>
								<field name="class_id" domain="[('standard_id','=',program),('semester_id','=',level_id),('school_id','=',campus),('state','=','running')]" widget="selection"/>
								<field name="date"/>
								<field name="strength"/>
								<field name="survey_student"/>

							</group>
						</group>
						<separator string="Course Survey"/>
						<field name="complete_lines">
							<tree editable="bottom" create='false' delete='false' edit='false'>
								<field name="s_no"/>
								<field name="question"/>
								<field name="s_agree"/>
								<field name="agree"/>
								<field name="neutral"/>
								<field name="disagree"/>
								<field name="strongly_disagree"/>
								<!-- <field name="frequency"/>
								<field name="re_frequency"/> -->
								<field name="percentage" string="High Percentage"/>
							</tree>
						</field>
						<separator string="Classes for the term"/>
						<field name="term_lines">
							<tree editable="bottom" create='false' delete='false' edit='false'>
								<field name="s_no"/>
								<field name="question"/>
								<field name="s_agree"/>
								<field name="agree"/>
								<field name="neutral"/>
								<field name="disagree"/>
								<field name="strongly_disagree"/>
								<!-- <field name="frequency"/>
								<field name="re_frequency"/> -->
								<field name="percentage"/>
							</tree>
						</field>
						<separator string="Materials"/>
						<field name="complete_material_lines">
							<tree editable="bottom" create='false' delete='false' edit='false'>
								<field name="s_no"/>
								<field name="question"/>
								<field name="s_agree"/>
								<field name="agree"/>
								<field name="neutral"/>
								<field name="disagree"/>
								<field name="strongly_disagree"/>
								<!-- <field name="frequency"/>
								<field name="re_frequency"/> -->
								<field name="percentage" string="High Percentage"/>
							</tree>
						</field>
						<separator string="Employee"/>
						<field name="complete_employee_lines">
							<tree editable="bottom" create='false' delete='false' edit='false'>
								<field name="s_no"/>
								<field name="question"/>
								<field name="s_agree"/>
								<field name="agree"/>
								<field name="neutral"/>
								<field name="disagree"/>
								<field name="strongly_disagree"/>
								<!-- <field name="frequency"/>
								<field name="re_frequency"/> -->
								<field name="percentage" string="High Percentage"/>
							</tree>
						</field>
						<separator string="Campus Environment"/>
						<field name="complete_environment_lines">
							<tree editable="bottom" create='false' delete='false' edit='false'>
								<field name="s_no"/>
								<field name="question"/>
								<field name="s_agree"/>
								<field name="agree"/>
								<field name="neutral"/>
								<field name="disagree"/>
								<field name="strongly_disagree"/>
								<!-- <field name="frequency"/>
								<field name="re_frequency"/> -->
								<field name="percentage" string="High Percentage"/>
							</tree>
						</field>
						<separator string="Trainers"/>
						<field name="complete_trainer_lines">
							<tree editable="bottom" create='false' delete='false' edit='false'>
								<field name="s_no"/>
								<field name="question"/>
								<field name="s_agree"/>
								<field name="agree"/>
								<field name="neutral"/>
								<field name="disagree"/>
								<field name="strongly_disagree"/>
								<!-- <field name="frequency"/>
								<field name="re_frequency"/> -->
								<field name="percentage" string="High Percentage"/>
							</tree>
						</field>
					</sheet>
				</form>
			</field>
		</record>


	<record id="complete_survey_form_action" model="ir.actions.act_window">
		<field name="name">Survey Form</field>
		<field name="res_model">complete.survey</field>
		<field name="view_mode">tree,form</field>
	</record>

	<menuitem
		id="complete_survey_menuitem"
		parent='satisfaction_menus'
		action="complete_survey_form_action"
		name="Complete Survey Form"/>
	</data>
</odoo>